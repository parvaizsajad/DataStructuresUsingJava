		int[] arr= {1,4,2,3,3,5};
		int index=0;
		int peek= arr[0];
		int n=6;
		int i=0;
		while (n <6) {
			if(arr[i]>peek) {
				peek=arr[i];
				index++;
			}else {
				index--;
			}
		}
		System.out.println("the peek is:"+ peek +"\t the index is "+ index);
	}

