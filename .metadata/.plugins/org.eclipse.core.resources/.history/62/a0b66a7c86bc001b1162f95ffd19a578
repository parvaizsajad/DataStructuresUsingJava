package ll;



public class LL {
	public class Node{
		Node next;
		Node prev;
		int data;
	public Node(int data) {
		this.data=data;
	}
	}
Node head;
Node tail;
int size=0;
	
	
	@Override
public String toString() {
		StringBuffer sb=new StringBuffer();
		sb.append("[");
		Node curr=head;
		while(curr!=null) {
			sb.append(curr.data).append(",");
			curr=curr.next;
			if(sb.length()>1)
			{
				sb.deleteCharAt(sb.length()-1);
			}
			sb.append("]");
		}
	return sb.toString();
}

	public void add(int data) {
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
LL ll=new LL();
ll.add(2);
ll.add(3);
ll.add(4);
System.out.println(ll.size);
	}

}
